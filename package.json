  "scripts": {
    "start": "vite",
    "build": "vite build",
    "type-check": "tsc",
    "lint": "eslint src --ext .ts,.tsx --max-warnings=0",
    "lintfix": "eslint ./ --ext .ts,.tsx --fix",
    "prettier": "prettier --write ./src",
    "precommit": "lint-staged && npm run type-check && npm run lint",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "*.{js, jsx, tsx, ts, scss, json}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm precommit"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@date-io/date-fns-jalali": "^3.0.0",
