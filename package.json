    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^6.0.0",
    "lint-staged": "^13.0.1",
    "prettier": "^2.8.1",
    "prettier-eslint": "^12.0.0",
    "typescript": "^4.9.4",
